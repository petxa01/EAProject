<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="EAHibernate.Develops" table="develops" schema="ea">
        <composite-id mapped="true" class="EAHibernate.DevelopsPK">
            <key-property name="developerId">
                <column name="DeveloperId" sql-type="int(11)"/>
            </key-property>
            <key-property name="gameId">
                <column name="GameId" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="finished">
            <column name="Finished" sql-type="tinyint(4)"/>
        </property>
        <many-to-one name="developersByDeveloperId" class="EAHibernate.Developers" insert="false" update="false">
            <column name="DeveloperId"/>
        </many-to-one>
        <many-to-one name="gamesByGameId" class="EAHibernate.Games" insert="false" update="false">
            <column name="GameId"/>
        </many-to-one>
    </class>
</hibernate-mapping>