<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="EAHibernate.Games" table="games" schema="ea">
        <id name="id" >
            <column name="Id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="genre">
            <column name="Genre" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="float" precision="-1"/>
        </property>
        <property name="franchiseId" insert="false" update="false">
            <column name="FranchiseId" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="franchisesByFranchiseId" class="EAHibernate.Franchises">
            <column name="FranchiseId" not-null="true"/>
        </many-to-one>
        <set name="developsById" inverse="true">
            <key>
                <column name="GameId"/>
            </key>
            <one-to-many not-found="ignore" class="EAHibernate.Develops"/>
        </set>
        <set name="salesById" inverse="true">
            <key>
                <column name="GameId" not-null="true" />
            </key>
            <one-to-many not-found="ignore" class="EAHibernate.Sales"/>
        </set>
    </class>
</hibernate-mapping>