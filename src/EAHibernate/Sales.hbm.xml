<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="EAHibernate.Sales" table="sales" schema="ea">
        <id name="id">
            <column name="Id" sql-type="int(11)"/>
        </id>
        <property name="quantity">
            <column name="Quantity" sql-type="int(11)"/>
        </property>
        <property name="date">
            <column name="Date" sql-type="date" not-null="true"/>
        </property>
        <property name="gameId">
            <column name="GameId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="ClientId" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="gamesByGameId" class="EAHibernate.Games">
            <column name="GameId" not-null="true"/>
        </many-to-one>
        <many-to-one name="clientsByClientId" class="EAHibernate.Clients">
            <column name="ClientId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>